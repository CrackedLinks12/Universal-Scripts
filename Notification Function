getgenv().Notify = function(title, text, icon, time)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = title; 
        Text = text;
        Icon = icon;
        Duration = time;
    })
end
Notify("Title here", "Text here", "Icon assetid here", 5) -- Can be any number (how long the notification lasts)
